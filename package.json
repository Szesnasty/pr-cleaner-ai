{
    "name": "pr-cleaner-ai",
    "version": "1.2.12",
    "description": "Cursor + GitHub PR cleanup assistant. Local dev tool (install as devDependency).",
    "bin": {
        "pr-cleaner-ai": "dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "postinstall": "node scripts/postinstall.js",
        "prepublishOnly": "npm run build && npm run clean:maps",
        "clean:maps": "find dist -name '*.map' -delete"
    },
    "files": [
        "bin/",
        "dist/",
        "scripts/",
        "config/pr-cleaner-ai.mdc",
        "README.md",
        "LICENSE",
        "CHANGELOG.md",
        "SECURITY.md",
        "CODE_OF_CONDUCT.md",
        "CONTRIBUTING.md"
    ],
    "keywords": [
        "cursor",
        "cursor-ai",
        "github-cli",
        "gh-cli",
        "pull-request",
        "github-pr",
        "pr-comments",
        "code-review",
        "review-comments",
        "code-review-automation",
        "automation",
        "developer-tools",
        "developer-productivity",
        "devtool",
        "merge-ready",
        "pr-cleanup",
        "github-integration"
    ],
    "author": {
        "name": "Åukasz Jakubowski",
        "email": "lukasz.jakubowski1993@gmail.com",
        "url": "https://github.com/Szesnasty"
    },
